<body class="iframeBody" style="background-color: #ffffff;">
	<section>
		<div class="main-content-child">
			<div class="nav" id="nav"></div>
			<div class="panel-body">
				<div class="col-md-12"
					style="border: 1px solid #e7ecf1; margin-top: 30px; margin-bottom: 30px; padding-bottom: 20px;">
					<div class="nav">
						<ol class="breadcrumb" style="margin-left: -15px;margin-right: -15px;">
							<li class="tablename">保证金表</li>
						</ol>
					</div>
					<div class="whiteBgDiv row">
						<form class="form-horizontal" role="form" id="searchForm">
							<div class="form-group col-sm-12">
								<label class="col-sm-2 control-label" for="financeId">融资编号</label>
								<div class="col-sm-3">
									<input type="text" name="financeId" class="form-control"
										id="financeId">
								</div>
								<label class="col-sm-2 control-label" for="guaranteePayDate">缴纳日期</label>
								<div class="col-sm-3 date form_datetime">
									<input name="guaranteePayDate" class="form-control addInput"
										readonly id="guaranteePayDate" type="text"> <span
										class="glyphicon glyphicon-calendar addIcon"> </span>
								</div>
								<input type="button" style="margin: auto;"
									onclick="$('#guaranteePayDate').val('');"
									class="btn btn-default cancel" value="清空时间">
							</div>
							<div class="form-group col-sm-12" id="divAgencyName">
								<label class="col-sm-2 control-label" for="agencyName">客户名称</label>
								<div class="col-sm-3">
									<input type="text" name="agencyName" class="form-control"
										id="agencyName">
								</div>
							</div>
						</form>
						<!-- <div class="col-sm-offset-10 col-sm-12">
		                    <a onclick="searchFun()" class="btn btn-primary search">查找</a>
		                    <a onclick="exportExcel()" class="btn btn-default cancel">导出Excel</a>
		                </div> -->
						<div class="col-sm-12">
							<input type="button" onclick="exportExcel()"
								class="btn btn-default cancel pull-right"
								style="margin-right: 5px;" value="导出Excel"> <input
								type="submit" onclick="searchFun()"
								class="btn btn-default save pull-right" id="btnSave"
								style="margin-right: 5px;" value="查找">
						</div>
					</div>
					<table id="marginListTable" class="table" cellspacing="0"
						width="100%">
					</table>
				</div>
			</div>
		</div>
	</section>
</body>

</html>