<form class="form-horizontal" role="form" id="detailAddForm"  data-toggle="validator">
     <div class="modal fade" id="detailAddModal" aria-hidden="true" data-backdrop="static" tabindex="-1" role="dialog" aria-labelledby="detailAddModalLabel">
  <div class="modal-dialog modal-lg" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span> </button> 
        <h4 class="modal-title" id="detailAddModalLabel">经销商信息</h4>
      </div>
      <div class="modal-body">
        <div class="form-group">
          <label class="col-sm-2 control-label" for="corpName">经销商名称</label>
          <div class="col-sm-3">
          	<input type="text" name="corpName" class="form-control" id="corpName" readonly>
          </div>
          <label class="col-sm-3 control-label"  for="agencyNum">经销商代码</label>
          <div class="col-sm-3">
          	<input type="text" name="agencyNum" class="form-control" id="agencyNum" readonly>
          </div>
        </div>
        <div class="form-group">
          <label class="col-sm-2 control-label"  for="maxCreditAmount">最高授信额度</label>
          <div class="col-sm-3">
          	<input type="text" name="maxCreditAmount" class="form-control" id="maxCreditAmount" readonly>
        	</div>
          <label class="col-sm-3 control-label"  for="officeAddress">公司地址</label>
        	<div class="col-sm-3">
          	<input type="text" name="officeAddress" class="form-control" id="officeAddress" readonly>
        	</div>
        </div>
        <div class="form-group">
          <label class="col-sm-2 control-label"  for="contactInfo">联系方式</label>
        	<div class="col-sm-3">
          	<input type="text" name="contactInfo" class="form-control" id="contactInfo" readonly>
        	</div>
          <label class="col-sm-3 control-label"  for="fixedPhone">固定电话</label>
        	<div class="col-sm-3">
          	<input type="text" name="fixedPhone" class="form-control" id="fixedPhone" readonly>
        	</div>
        </div>
        <div class="form-group">
          <label class="col-sm-2 control-label"  for="area">所属区域</label>
        	<div class="col-sm-3">
          	<select id="area" name="area" class="form-control">
          		<option value=""></option>
              	<option value="0">东区事业部</option>
              	<option value="1">西区事业部</option>
              	<option value="2">南区事业部</option>
              	<option value="3">北区事业部</option>
              </select>
        	</div>
          <label class="col-sm-3 control-label"  for="represent">所属商代处</label>
        	<div class="col-sm-3">
          	<select id="represent" name="represent" class="form-control">
              </select>
        	</div>
        </div>
        <div class="form-group">
          <label class="col-sm-2 control-label"  for="firstTwoYearsPickupNum">前2年度提车数量</label>
        	<div class="col-sm-3">
          	<input type="text" name="firstTwoYearsPickupNum" class="form-control" id="firstTwoYearsPickupNum" readonly>
        	</div>
          <label class="col-sm-3 control-label"  for="firstTwoYearsRetailNum">前2年度零售数量</label>
        	<div class="col-sm-3">
          	<input type="text" name="firstTwoYearsRetailNum" class="form-control" id="firstTwoYearsRetailNum" readonly>
        	</div>
        </div>
        <div class="form-group">
          <label class="col-sm-2 control-label"  for="firstTwoYearsSaleRank">前2年度销售排名</label>
        	<div class="col-sm-3">
          	<input type="text" name="firstTwoYearsSaleRank" class="form-control" id="firstTwoYearsSaleRank" readonly>
        	</div>
          <label class="col-sm-3 control-label"  for="thisYearPlanPickupNum">本年度计划提车数量</label>
        	<div class="col-sm-3">
          	<input type="text" name="thisYearPlanPickupNum" class="form-control" id="thisYearPlanPickupNum" readonly>
        	</div>
        </div>
        <div class="form-group">
          <label class="col-sm-2 control-label"  for="thisYearPlanSales">本年度计划销售额</label>
        	<div class="col-sm-3">
          	<input type="text" name="thisYearPlanSales" class="form-control" id="thisYearPlanSales" readonly>
        	</div>
          <label class="col-sm-3 control-label"  for="note">备注</label>
        	<div class="col-sm-3">
          	<input type="text" name="note" class="form-control" id="note" readonly>
        	</div>
        </div>
      </div>
     <div class="modal-footer">
  <div class="form-group">
	<div class="col-sm-2 col-sm-offset-8">
		<button type="button" class="btn btn-default cancel"
			data-dismiss="modal">取消
		</button>
	</div>
	</div>
   	 </div>
   </div>
 </div>
 </div>
</form>