                        <div class="col-sm-3 scf_valid">
                            <input type="text" name="recOrderNum" class="form-control" id="recOrderNumDetail" placeholder="接受订单数量" readonly>
                        </div>
                        <label class="col-sm-2 control-label" for="recOrderAmtDetail">接受订单金额</label>
                        <div class="col-sm-3 scf_valid">
                            <input type="text" name="recOrderAmt" class="form-control" id="recOrderAmtDetail" placeholder="接受订单金额" readonly>
                        </div>
                    </div>
                    <div class="form-group">
                
                        <label class="col-sm-2 control-label" for="loanAmtDetail">放款金额</label>
                        <div class="col-sm-3 scf_valid">
                            <input type="text" name="crReqTotalAmt" class="form-control" id="loanAmtDetail" placeholder="放款金额" readonly>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-sm-2 control-label" for="syncDateDetail">同步时间</label>
                        <div class="col-sm-3 scf_valid">
                            <input type="text" name="syncDate" class="form-control " id="syncDateDetail" placeholder="同步时间" readonly>
                            
                        </div>
                        <label class="col-sm-2 control-label" for="selDateDetail">确认时间</label>
                        <div class="col-sm-3 scf_valid">
                            <input type="text" name="selDate" class="form-control " id="selDateDetail" placeholder="确认时间" readonly>
                            
                        </div>
                    </div>
                </form>
            </div>
            <div class="panel-body">
                <div class="nav">
                    <ol class="breadcrumb">
