#repayInfo .th-inner {
	height: 80px;
}
</style>

<body class="iframeBody" style="background-color: #ffffff;">
	<section>
		<div class="main-content-child">
			<div class="nav" id="nav"></div>
			<div class="col-md-12" style="margin-top: 30px;margin-bottom: 30px;padding-bottom: 20px;border: 1px solid #e7ecf1;">
				<div class="nav">
					<ol class="breadcrumb" style="margin-left: -15px;margin-right: -15px;">
						<li class="tablename">应收账款明细表</li>
					</ol>
				</div>
				<div class="whiteBgDiv row">
					<form class="form-horizontal" role="form" id="searchForm">
						<div class="form-group col-sm-12">
							<label class="col-sm-2 control-label" for="corpName">经销商名称</label>
							<div class="col-sm-3">
								<input class="form-control" id="corpName" name="corpName"
									type="text" placeholder="经销商名称" />
							</div>
							<label class="col-sm-2 control-label" for="agencyNum">经销商代码</label>
							<div class="col-sm-3">
								<input name="agencyNum" class="form-control" id="agencyNum"
									type="text" placeholder="经销商代码">
							</div>
						</div>
					</form>
					<div class="col-sm-12">
						<input type="submit" onclick="searchFun()"
							class="btn btn-default save pull-right" id="btnSave"
							style="margin-right: 20px;" value="查找">
					</div>
				</div>
				<div class="panel-body">
					<table id="ARInfoListTable" class="table" cellspacing="0"
						width="100%">
					</table>
				</div>
			</div>
		</div>
	</section>
</body>

