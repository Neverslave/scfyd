<form class="form-horizontal" role="form" id="addForm" data-toggle="validator">
	<div class="form-group">
		<label class="col-sm-2 control-label" for="loanDate"><span class="required">*</span>放款日期</label>
		<div class="col-sm-3">
			<input name="loanDate" class="form-control" id="loanDate" readonly="readonly" type="text">
		</div>
		<label class="col-sm-2 control-label" for="contractNo"><span class="required">*</span>合同编号</label>
		<div class="col-sm-3">
			<input type="text" name="contractNo" class="form-control" id="contractNo" readonly="readonly">
		</div>
	</div>
	<div class="form-group">
		<label class="col-sm-2 control-label" for="orderBatchId"><span class="required">*</span>订单批次号</label>
		<div class="col-sm-3">
			<input type="text" name="orderBatchId" class="form-control" id="orderBatchId" readonly="readonly">
		</div>
		<label class="col-sm-2 control-label" for="orderAcceptMoney"><span class="required">*</span>接收订单金额</label>
		<div class="col-sm-3">
			<input type="text" name="orderAcceptMoney" class="form-control" id="orderAcceptMoney" readonly="readonly">
		</div>
	</div>
	<div class="form-group">
		<label class="col-sm-2 control-label" for="orderAllMoney"><span class="required">*</span>超人所需费用总额</label>
		<div class="col-sm-3">
			<input type="text" name="orderAllMoney" class="form-control" id="orderAllMoney" readonly="readonly">
		</div>
		<label class="col-sm-2 control-label" for="loanAmt"><span class="required">*</span>放款金额</label>
		<div class="col-sm-3">
			<input type="text" name="loanAmt" class="form-control" id="loanAmt" readonly="readonly">
		</div>
	</div>
	<div class="form-group">
		<label class="col-sm-2 control-label" for="financeStartDay"><span class="required">*</span>融资起始日</label>
		<div class="col-sm-3">
			<input name="financeStartDay" class="form-control" id="financeStartDay" readonly="readonly" type="text">
		</div>
		<label class="col-sm-2 control-label" for="financeDueDay"><span class="required">*</span>融资到期日</label>
		<div class="col-sm-3">
			<input name="financeDueDay" class="form-control" id="financeDueDay" type="text" readonly="readonly">
		</div>
	</div>
	<div class="form-group">
		<label class="col-sm-2 control-label" for="guaranteeMoneyRate"><span class="required">*</span>保证金比例(%)</label>
		<div class="col-sm-3">
			<input type="text" name="guaranteeMoneyRate" class="form-control" id="guaranteeMoneyRate" readonly="readonly">
		</div>
		<label class="col-sm-2 control-label" for="guaranteeMoney"><span class="required">*</span>保证金金额</label>
		<div class="col-sm-3">
			<input type="text" name="guaranteeMoney" class="form-control" id="guaranteeMoney" readonly="readonly">
		</div>
	</div>
	<div class="form-group">
		<label class="col-sm-2 control-label" for="guaranteeMoneyActual"><span class="required">*</span>实缴保证金</label>
		<div class="col-sm-3 valid_group">
			<input type="text" name="guaranteeMoneyActual" class="form-control" id="guaranteeMoneyActual">
		</div>
		<label class="col-sm-2 control-label" for="note"><span class="required">*</span>备注</label>
		<div class="col-sm-3 valid_group">
			<textarea id="note" name="note" class="form-control"></textarea>
		</div>
	</div>
	<div class="form-group">
	    <label class="col-sm-2 control-label"  for="advice">审核结果</label>
	    <div class="col-sm-3">
	    	<textarea id="advice"  name="advice" class="form-control" readonly></textarea>
	    </div>
    </div>
</form>