<form class="form-horizontal" role="form" id="addFormHis" data-toggle="validator">
	<div class="form-group">
		<label class="col-sm-2 control-label" for="loanDate">放款日期</label>
		<div class="col-sm-3">
			<input name="loanDate" class="form-control" id="loanDate" readonly="readonly" type="text">
		</div>
		<label class="col-sm-2 control-label" for="contractNo">合同编号</label>
		<div class="col-sm-3">
			<input type="text" name="contractNo" class="form-control" id="contractNo" readonly="readonly">
		</div>
	</div>
	<div class="form-group">
		<label class="col-sm-2 control-label" for="orderBatchId">订单批次号</label>
		<div class="col-sm-3">
			<input type="text" name="orderBatchId" class="form-control" id="orderBatchId" readonly="readonly">
		</div>
		<label class="col-sm-2 control-label" for="orderAcceptMoney">接收订单金额</label>
		<div class="col-sm-3">
			<input type="text" name="orderAcceptMoney" class="form-control" id="orderAcceptMoney" readonly="readonly">
		</div>
	</div>
	<div class="form-group">

		<label class="col-sm-2 control-label" for="loanAmt">放款金额</label>
		<div class="col-sm-3">
			<input type="text" name="loanAmt" class="form-control" id="loanAmt" readonly="readonly">
		</div>
	</div>
	<div class="form-group">
		<label class="col-sm-2 control-label" for="financeStartDay">融资起始日</label>
		<div class="col-sm-3">
			<input name="financeStartDay" class="form-control" id="financeStartDay" readonly="readonly" type="text">
		</div>
		<label class="col-sm-2 control-label" for="financeDueDay">融资到期日</label>
		<div class="col-sm-3">
			<input name="financeDueDay" class="form-control" id="financeDueDay" type="text" readonly="readonly">
		</div>
	</div>
	<div class="form-group">
		<label class="col-sm-2 control-label" for="guaranteeMoneyRate">保证金比例(%)</label>
		<div class="col-sm-3">
			<input type="text" name="guaranteeMoneyRate" class="form-control" id="guaranteeMoneyRate" readonly="readonly">
		</div>
		<label class="col-sm-2 control-label" for="guaranteeMoney">保证金金额</label>
		<div class="col-sm-3">
			<input type="text" name="guaranteeMoney" class="form-control" id="guaranteeMoney" readonly="readonly">
		</div>
	</div>
	<div class="form-group">
		<label class="col-sm-2 control-label" for="guaranteeMoneyActual">实缴保证金</label>
		<div class="col-sm-3 valid_group">
			<input type="text" name="guaranteeMoneyActual" class="form-control" id="guaranteeMoneyActual" readonly>
		</div>
		<label class="col-sm-2 control-label" for="note">备注</label>
		<div class="col-sm-3 valid_group">
			<textarea id="note" name="note" class="form-control" readonly></textarea>
		</div>
	</div>
	<div class="form-group agrAndAdvHis">
		<label class="col-sm-2 control-label" for="agree">审核结果</label>
		<div class="col-sm-3">
             <select class="form-control" id="agree" name="agree" disabled>
             	<option value="0">同意</option>
             	<option value="1">拒绝</option>
             </select>
         </div>
	    <label class="col-sm-2 control-label"  for="advice">审核意见</label>
	    <div class="col-sm-3">
	    	<textarea id="advice"  name="advice" class="form-control" readonly></textarea>
	    </div>
    </div>
</form>